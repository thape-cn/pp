<div class="modal-dialog modal-sm">
  <div class="modal-content">
    <%= form_with model: @calibration_session_user, url: admin_calibration_session_user_path(id: @calibration_session_user.id), method: :delete do |f| %>
      <div class="modal-header">
        <h5 class="modal-title"><%= t(".title") -%></h5>
        <button class="btn-close" type="button" data-coreui-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <p><%= t(".confirm", name: @calibration_session_user.user.chinese_name, title: @calibration_session_user.calibration_session.calibration_template.company_evaluation_template.title) -%></p>
      </div>
      <div class="modal-footer">
        <%= f.submit t('form.confirm'), class: 'btn btn-primary', data: {'coreui-dismiss': :modal} %>
        <button class="btn btn-secondary" type="button" data-coreui-dismiss="modal"><%= t("form.close") -%></button>
      </div>
    <% end %>
  </div>
</div>
