<div class="modal-dialog">
  <div class="modal-content">
    <%= form_with model: @company_evaluation, url: to_end_evaluation_admin_company_evaluation_path(id: @company_evaluation.id), method: :put do |f| %>
      <div class="modal-header">
        <h5 class="modal-title"><%= t(".title") -%></h5>
        <button class="btn-close" type="button" data-coreui-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
          <p><%= t('.confirm_explain_text') -%></p>
        </div>
      </div>
      <div class="modal-footer">
        <%= f.submit t('form.confirm'), class: 'btn btn-primary', data: {'coreui-dismiss': :modal} %>
        <button class="btn btn-secondary" type="button" data-coreui-dismiss="modal"><%= t("form.close") -%></button>
      </div>
    <% end %>
  </div>
</div>
